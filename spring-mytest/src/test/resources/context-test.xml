<?xml version="1.0" encoding="UTF-8"?>
<beans profile="dev"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.springframework.org/schema/beans" xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="context.component.BeanDependComponent" id="dependComponent">
		<constructor-arg index="0" value="depend"/>
		<property name="myTestBean" ref="myTestBean"/>
	</bean>
	<bean id="myTestBean" class="context.component.MyTestBean">
		<!--测试meta标签-->
		<meta key="special-data" value="sprecial stragey"/>
		<property name="name" value="dbw"/>
		<property name="beanDependComponent" ref="dependComponent"/>
	</bean>
	<alias name="myTestBean" alias="testBean"/>

	<bean id="factoryBeanTest" class="context.factorybean.MyBeanFactory">
		<property name="name" value="testFactory"/>
	</bean>
</beans>